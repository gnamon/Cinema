<form action="/adminaddseatprices" method="post">
    <div class="container-fluid">
        <div class="seat-form">
            <div class="seat-form__inner">
                <h4>Add Seat Price</h4>
                <div class="seatprice-input">
                    <label for="branch" class="seatprice-input__label">Branch</label>
                    <select name="Branch" required>
                        <option value="">- Please select Branch -</option>
                        <% Branches.forEach(function(Branch){ %>
                            <option value="<%=Branch.branchID%>"><%=Branch.branchName%></option>
                        <% }); %> 
                    </select>
                </div>
                <div class="seatprice__row">
                    <label for="theatertype" class="seatprice-input__label">Theater type</label>
                    <select name="TheathertypeID" required>
                        <option value="">- Please select Theathertype  -</option>
                        <% Theatertypes.forEach(function(TheaterType){ %>
                            <option value="<%=TheaterType.theaterTypeID%>"><%=TheaterType.TheaterTypeName%></option>
                        <% }); %> 
                    </select>
                <br><br><br>
                <div class="seatprice-btn">
                    <button class="save-btn">
                    Next
                    </button>
                </div>
            </div>
        </div>
    </div>
 </form>  
</div>